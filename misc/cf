
  #  $0 : the scriptname itself
  #  $# : Total number of arguments
  #  $1 : first argument
  #  $2 : second argument

var1=$1
var2=$2
echo $0
echo $#
echo $var1 $var2

sudo curl https://cdore00.github.io/golf/$var1 >$var1

# if [ var2 != "" ]

if [ -n "$var2" ]  # This time, $string1 is quoted.
then
  echo "String \"string1\" is not null."
else  
  echo "String \"string1\" is null."
fi 

exit 0
